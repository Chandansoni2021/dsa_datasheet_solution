//User function Template for Java

class Solution
{ 
    public static int findSum(int A[],int N) 
    {
        int z=A[0];
        int d=A[0];
        for(int i=0;i<A.length;i++){
           if(z<A[i]){
              z=A[i];
           }
           if(d>A[i]){
               d=A[i];
        }
        }
        return d+z;
       
    }


}
